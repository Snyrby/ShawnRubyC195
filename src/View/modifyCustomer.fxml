<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyCustomerController">
   <children>
      <VBox alignment="CENTER_RIGHT" prefHeight="500.0" prefWidth="425.0">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Customer ID:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="customerIDTextField" disable="true" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="First Name:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="firstNameTextField" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin></HBox>
            <Label fx:id="firstNameLabel" text="ENTER A VALID FIRST NAME" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="BOTTOM_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="90.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Last Name:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="lastNameTextField" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin></HBox>
            <Label fx:id="lastNameLabel" text="ENTER A VALID LAST NAME" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="BOTTOM_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Address:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="addressTextField" />
               </children></HBox>
            <Label fx:id="addressLabel" text="ENTER A VALID ADDRESS" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Country:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="countryComboBox" prefWidth="150.0" />
               </children>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></HBox>
            <Label fx:id="countryLabel" text="SELECT A VALID COUNTRY!" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="State/Province:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="stateComboBox" prefWidth="150.0" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin></HBox>
            <Label fx:id="stateLabel" text="SELECT A VALID STATE/PROVINCE!" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="BOTTOM_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Phone Number:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="phoneNumberTextField" />
               </children>
               <VBox.margin>
                  <Insets top="-10.0" />
               </VBox.margin></HBox>
            <Label fx:id="phoneNumberLabel" text="ENTER A VALID PHONE NUMBER" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="BOTTOM_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Zip Code:">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="zipCodeTextField" />
               </children>
            </HBox>
            <Label fx:id="zipCodeLabel" text="ENTER A VALID ZIPCODE" textFill="#ff2805" visible="false">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_RIGHT" maxHeight="42.4" minHeight="42.4" prefHeight="42.4" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#updateCustomerButtonPushed" text="Change Customer" />
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelButtonPushed" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
